<?xml version="1.0" encoding="UTF-8"?>

<section xml:id="sage-cell" xmlns:xi="http://www.w3.org/2001/XInclude">
  <title>Sage Cell</title>

  <p> This is a sage cell. Run some sage code! </p>

  <sage>
    <input>
      # Creating sets
      A = Set([1, 2, 3])
      B = Set([3, 4, 5])
      C = Set([])  # An empty set

      # Display the sets
      print("Set A:", A)
      print("Set B:", B)
      print("Set C:", C)
    </input>

    <output></output>
  </sage>

  <sage>
    <input>
      # Union of A and B
      union_AB = A.union(B)
      print("Union of A and B:", union_AB)
    </input>

    <output></output>
  </sage>

  <sage>
    <input>
      # Intersection of A and B
      intersection_AB = A.intersection(B)
      print("Intersection of A and B:", intersection_AB)
    </input>

    <output></output>
  </sage>

  <sage>
    <input>
      # Difference of A and B (elements in A that are not in B)
      difference_AB = A.difference(B)
      print("Difference of A and B (A - B):", difference_AB)
    </input>

    <output></output>
  </sage>

  <sage>
    <input>
      # Checking subsets and supersets
      print("Is A a subset of B?", A.issubset(B))
      print("Is A a superset of C?", A.issuperset(C))
    </input>

    <output></output>
  </sage>

  <sage>
    <input>
      # Cardinality (number of elements in the set)
      print("Number of elements in Set A:", A.cardinality())
    </input>

    <output></output>
  </sage>





</section>